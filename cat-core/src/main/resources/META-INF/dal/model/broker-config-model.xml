<?xml version="1.0" encoding="UTF-8"?>
<model model-package="com.dianping.cat.configuration.app.broker" enable-sax-parser="true">
  <entity name="broker-config" root="true">
    <attribute name="group-statistics-enabled" value-type="boolean" primitive="true" />
    <attribute name="remote-ip-service-enabled" value-type="boolean" primitive="true"/>
    <attribute name="dev-mode" value-type="boolean" primitive="true" />
    <entity-ref name="storge" />
    <entity-ref name="too-long-command" />
  </entity>
  <entity name="storge">
    <entity-ref name="local-access-log" />
    <entity-ref name="local-error-log" />
    <entity-ref name="checkpoint" />
  </entity>
  <entity name="local-access-log">
    <attribute name="path" value-type="String" default-value="/data/applogs/broker-service/logs"/>
    <attribute name="duration-in-hour" value-type="int" primitive="true" default-value="6"/>
    <entity-ref name="type" type="map" names="types" />
  </entity>
  <entity name="type">
    <attribute name="id" value-type="String" key="true"/>
    <attribute name="enabled" value-type="boolean" primitive="true"/>
  </entity>
  <entity name="local-error-log">
    <attribute name="path" value-type="String" default-value="/data/appdatas/cat/app-error-log"/>
    <attribute name="duration-in-day" value-type="int" primitive="true" default-value="15"/>
    <attribute name="enabled" value-type="boolean" primitive="true"/>
  </entity>
  <entity name="checkpoint">
     <attribute name="local-flush-enabled" value-type="boolean" primitive="true" />
    <entity-ref name="local-flush" />
  </entity>
  <entity name="local-flush">
    <attribute name="path" value-type="String" default-value="/data/appdatas/cat/app-data-save"/>
  </entity>
  <entity name="too-long-command">
    <attribute name="id" value-type="String" defalut-value="toolongurl.bin"/>
  </entity>
</model>

