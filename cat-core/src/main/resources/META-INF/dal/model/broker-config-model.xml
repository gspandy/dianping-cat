<?xml version="1.0" encoding="UTF-8"?>
<model model-package="com.dianping.cat.configuration.app.broker" enable-sax-parser="true">
   <entity name="broker-config" root="true">
    <attribute name="group-statistics-enabled" value-type="boolean" primitive="true" />
    <attribute name="remote-ip-service-enabled" value-type="boolean" primitive="true" />
    <attribute name="dev-mode" value-type="boolean" primitive="true" />
    <entity-ref name="type" type="map" names="logs" xml-indent="true" />
    <entity-ref name="checkpoint" />
    <entity-ref name="too-long-command" />
  </entity>
  <entity name="type">
    <attribute name="id" value-type="String" key="true"/>
    <attribute name="path" value-type="String" />
    <attribute name="duration-in-hours" value-type="int" primitive="true" />
    <attribute name="enabled" value-type="boolean" primitive="true" />
  </entity>
  <entity name="checkpoint">
    <attribute name="local-flush-enabled" value-type="boolean" primitive="true" />
    <entity-ref name="local-flush" />
  </entity>
  <entity name="local-flush">
    <attribute name="path" value-type="String" />
  </entity>
  <entity name="too-long-command">
    <attribute name="id" value-type="String" />
  </entity>
</model>

