<?xml version="1.0" encoding="UTF-8"?>
<model model-package="com.dianping.cat.home.screen" enable-sax-parser="true">
  <entity name="screen-config" root="true">
    <entity-ref name="screen" type="map" names="screens" />
  </entity>
  <entity name="screen">
    <attribute name="id" value-type="String" key="true" />
    <attribute name="title" value-type="String" />
    <entity-ref name="graph" type="map" names="graphs" />
  </entity>
  <entity name="graph">
    <attribute name="id" value-type="String" key="true" />
    <attribute name="title" value-type="String" />
    <entity-ref name="item" type="map" names="items" />
  </entity>
  <entity name="item">
    <attribute name="id" value-type="String" key="true" />
    <attribute name="title" value-type="String" />
    <entity-ref name="segment" type="map" names="segments" />
  </entity>
  <entity name="segment">
    <attribute name="id" value-type="String" key="true" />
    <attribute name="title" value-type="String" />
    <attribute name="category" value-type="String" />
    <attribute name="measure" value-type="String" />
    <attribute name="tags" value-type="String" />
    <attribute name="endpoint" value-type="String" />
    <attribute name="type" value-type="String" />
  </entity>
</model>

