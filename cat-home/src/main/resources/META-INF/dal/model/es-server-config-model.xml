<?xml version="1.0" encoding="UTF-8"?>
<model model-package="com.dianping.cat.home.eslog"  enable-merger="true"  enable-sax-parser="true"
	enable-base-visitor="true"  enable-xml-parser="false"  enable-xml-builder="false"  enable-native-parser="true"  enable-native-builder="true">
   <entity name="es-server-config" root="true">
    <entity-ref name="log" type="map" names="logs"  method-find-or-create='true'/>
  </entity>
  <entity name="log">
    <attribute name="id" value-type="String" key="true"/>
    <attribute name="host" value-type="String"/>
    <attribute name="port" value-type="String"/>
    <attribute name="index" value-type="String"/>
    <attribute name="type" value-type="String"/>
  </entity>
</model>
